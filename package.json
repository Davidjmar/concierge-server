{
  "name": "activity-recommender",
  "version": "1.0.0",
  "description": "AI-powered activity recommendation service",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon",
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -w",
    "test": "NODE_OPTIONS='--experimental-vm-modules --experimental-fetch' jest --config jest.config.js --detectOpenHandles",
    "test:watch": "jest --watch",
    "recommendations": "node --loader ts-node/esm src/scripts/triggerRecommendations.ts",
    "test:sheets": "node --loader ts-node/esm src/scripts/testGoogleSheets.ts",
    "populate": "node --loader ts-node/esm src/scripts/populateEvents.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.0",
    "axios": "^0.24.0",
    "cheerio": "^1.0.0-rc.10",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "googleapis": "^144.0.0",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.7.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "resend": "^4.1.2",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.15.11",
    "@types/node-cron": "^3.0.7",
    "@types/pg": "^8.11.0",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^2.0.12",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.1.6",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
